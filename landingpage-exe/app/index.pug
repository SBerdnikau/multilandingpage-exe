<!DOCTYPE html>
html(lang="en")
    head
        include includes/head.pug

    body
        div(class="container")
            div(class="wrapper")
               header(class="header")
                    div(class="header-panel")
                        div(class="panel-left")
                            div(class="panel-left-language")
                                include includes/language.pug
                                p(class="panel-left-language-name") English
                                include includes/arrow.pug
                            div(class="panel-left-call")
                                include includes/phone.pug
                                p(class="panel-left-language-name") +440 875369208
                            div(class="panel=left-email")
                                include includes/email.pug
                                p(class="panel-left-language-name")   info@sitename.com          
                        div(class="panel-right")
                            div(class="panel-right-register")
                                include includes/signIn.pug
                                p(class="panel-left-language-name") 
